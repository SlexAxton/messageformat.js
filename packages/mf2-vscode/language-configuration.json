{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["{", "}"],
    ["|", "|"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["{", "}"],
    ["|", "|"]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["{", "}"],
    ["|", "|"]
  ],
  "onEnterRules": [
    {
      "beforeText": "^[^\\t ].*=.*",
      "action": { "indent": "indent" }
    },
    {
      "beforeText": "^[\\t ]",
      "action": { "indent": "none" }
    }
  ],
  "wordPattern": "(\\\\.|[^\\t\\n\\r .\\[\\]=#])+"
}
